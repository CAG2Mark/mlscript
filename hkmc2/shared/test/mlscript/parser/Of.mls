:global
:p


f of ()
//│ |f| |of| |(||)|
//│ AST: List(App(Ident(f),Tup(List(UnitLit(false)))))

f of 1
//│ |f| |of| |1|
//│ AST: List(App(Ident(f),Tup(List(IntLit(1)))))

f of (1)
//│ |f| |of| |(|1|)|
//│ AST: List(App(Ident(f),Tup(List(IntLit(1)))))

f of 1, 2
//│ |f| |of| |1|,| |2|
//│ AST: List(App(Ident(f),Tup(List(App(Ident(,),Tup(List(IntLit(1), IntLit(2))))))))

f of 1, 2, 3
//│ |f| |of| |1|,| |2|,| |3|
//│ AST: List(App(Ident(f),Tup(List(App(Ident(,),Tup(List(IntLit(1), App(Ident(,),Tup(List(IntLit(2), IntLit(3)))))))))))

f of (1, 2, 3)
//│ |f| |of| |(|1|,| |2|,| |3|)|
//│ AST: List(App(Ident(f),Tup(List(App(Ident(,),Tup(List(IntLit(1), App(Ident(,),Tup(List(IntLit(2), IntLit(3)))))))))))

f of g(1)
//│ |f| |of| |g|(|1|)|
//│ AST: List(App(Ident(f),Tup(List(App(Ident(g),Tup(List(IntLit(1))))))))

f of g of 1
//│ |f| |of| |g| |of| |1|
//│ AST: List(App(Ident(f),Tup(List(App(Ident(g),Tup(List(IntLit(1))))))))

(f of g) of 1
//│ |(|f| |of| |g|)| |of| |1|
//│ AST: List(App(App(Ident(f),Tup(List(Ident(g)))),Tup(List(IntLit(1)))))

(f of g)(1)
//│ |(|f| |of| |g|)|(|1|)|
//│ AST: List(App(App(Ident(f),Tup(List(Ident(g)))),Tup(List(IntLit(1)))))

f of (g)(1)
//│ |f| |of| |(|g|)|(|1|)|
//│ AST: List(App(Ident(f),Tup(List(App(Ident(g),Tup(List(IntLit(1))))))))


